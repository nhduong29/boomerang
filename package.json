{
  "name": "boomerang-sandbox",
  "version": "1.0.5",
  "description": "streaming logs from getsandbox service to the browser",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "bin": "./bin/boomerang-sandbox",
  "dependencies": {
    "CBuffer": "0.1.4",
    "byline": "^5.0.0",
    "commander": "1.3.2",
    "connect": "2.11.0",
    "cookie": "0.1.0",
    "daemon-fix41": "1.1.2",
    "socket.io": "^1.7.0",
    "untildify": "^3.0.2"
  },
  "devDependencies": {},
  "scripts": {
    "lint": "eslint .",
    "test": "mocha -r should --reporter spec test/*.js",
    "pkg": "pkg --out-path=dist ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhduong29/boomerang.git"
  },
  "keywords": [
    "tail",
    "syslog",
    "realtime",
    "log",
    "boomerang",
    "boomerang-sandbox"
  ],
  "author": "Duong Nguyen <nhduong29@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhduong29/boomerang/issues"
  },
  "pkg": {
    "assets": [
      "preset/*.json",
      "web/**/*"
    ],
    "targets": [
      "node8-alpine-x64",
      "node8-linux-x64",
      "node8-macos-x64",
      "node8-win-x64"
    ]
  },
  "homepage": "https://github.com/nhduong29/boomerang#readme",
  "preferGlobal": true
}
